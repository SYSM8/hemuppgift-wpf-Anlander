<Window x:Class="Hemuppgift_WPFtemplate.Pages.DeckPanel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate.Pages"
        mc:Ignorable="d"
        Title="DeckPanel" Height="450" Width="800">
    <Grid>
        <Button Content="Top" VerticalAlignment="Top" Width="80"/>
        <Button Content="Bottom" VerticalAlignment="Bottom" Width="80"/>
        <Button Content="Left" VerticalAlignment="Center" HorizontalAlignment="Left" Width="80"/>
        <Button Content="Right" VerticalAlignment="Center" HorizontalAlignment="Right" Width="80"/>
        <Button Content="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Width="80" Click="Button_Click"/>
        <Image x:Name="img_trophy" Source="../Images/TrophyImage.jpg" Height="280" Panel.ZIndex="2" Visibility="Hidden" />
    </Grid>
</Window>
